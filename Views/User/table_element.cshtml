@{
    ViewBag.Title = "table_element";
    Layout = "~/Views/_LayoutPage1.cshtml";
}

<h1>Table Create</h1>
<h3>Employee Entry</h3>
<hr />
<div class="row">
    <div class="col-md-4">
        <from asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="from-group">
                <label asp-for="Id" class="control-label"></label>
                <imput asp-for="Id" class="from-control" />
                <span asp-validation-from="Id" class="text-danger"></span>
            </div>
            <div class="from-group">
                <label asp-for="PersonId" class="control-label"></label>
                <imput asp-for="PersonId" class="from-control" />
                <span asp-validation-from="PersonId" class="text-danger"></span>
            </div>
            <div class="from-group">
                <label asp-for="UserName" class="control-label"></label>
                <imput asp-for="UserName" class="from-control" />
                <span asp-validation-from="UserName" class="text-danger"></span>
            </div>
            <div class="from-group">
                <label asp-for="Password" class="control-label"></label>
                <imput asp-for="Password" class="from-control" />
                <span asp-validation-from="Password" class="text-danger"></span>
            </div>
            <div class="from-group">
                <label asp-for="IsAdministrator" class="control-label"></label>
                <imput asp-for="IsAdministrator" class="from-control" />
                <span asp-validation-from="IsAdministrator" class="text-danger"></span>
            </div>
            <div class="form-group">
                <imput type"submit" value="table_element" class="btn-success" />
            </div>
        </from>
    </div>
</div>

<div>
    <a asp-action="MenuUser"></a>
</div>
@section Scripts{
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}